[
    "group: 03_FF",
    {
        "description":"2Mbps(ethernet/ipv4/tcp)-->'in_port=1,actions=group:ff(watch_port=2,actions=output:2/watch_port=3,actions=output:3)'",
        "prerequisite":[
            {
                "OFPGroupMod":{
                    "group_id":0,
                    "type":3,
                    "buckets":[
                        {
                            "OFPBucket":{
                                "watch_port":2,
                                "actions":[
                                    {
                                        "OFPActionOutput":{
                                            "port":2
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "OFPBucket":{
                                "watch_port":3,
                                "actions":[
                                    {
                                        "OFPActionOutput":{
                                            "port":3
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            {
                "OFPFlowMod":{
                    "table_id":0,
                    "match":{
                        "OFPMatch":{
                            "oxm_fields":[
                                {
                                    "OXMTlv":{
                                        "field":"in_port",
                                        "value":1
                                    }
                                }
                            ]
                        }
                    },
                    "instructions":[
                        {
                            "OFPInstructionActions":{
                                "actions":[
                                    {
                                        "OFPActionGroup":{
                                            "group_id":0
                                        }
                                    }
                                ],
                                "type":4
                            }
                        }
                    ]
                }
            }
        ],
        "tests":[
            {
                "ingress":{
                    "packets":{
                        "data":[
                            "ethernet(dst='22:22:22:22:22:22', src='12:11:11:11:11:11', ethertype=2048)",
                            "ipv4(proto=6)",
                            "tcp()",
                            "str(\"\\x11\" * (1500 - 54))"
                        ],
                        "pktps":175,
                        "duration_time":30
                    }
                },
                "egress":{
                    "throughput":[
                        {
                            "OFPMatch":{
                                "oxm_fields":[
                                    {
                                        "OXMTlv":{
                                            "field":"in_port",
                                            "value":2
                                        }
                                    }
                                ]
                            },
                            "kbps":2000
                        }
                    ]
                }
            }
        ],
        "cleanup":[
            {
                "OFPPortMod":{
                    "port_no":2,
                    "config":1,
                    "mask":1
                }
            }
        ]
    },
